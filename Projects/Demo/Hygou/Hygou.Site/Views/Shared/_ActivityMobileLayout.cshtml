@{ 
    Layout = null;
    var setting = Html.Setting<CrowdMall.CrowdMallSetting>();
    var couponSetting = Html.Setting<ServiceProtocol.Biz.Promotion.Coupons.CouponSetting>();
}
<!DOCTYPE html>
<html lang="en">
<head>
	<title>@setting.SiteName</title>
	<meta charset="utf-8" />
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport" />
    <link rel="shortcut icon" href="/favicon.ico" />
    @Html.CDNStyle("~/bundles/mobile/activity/css")

    @RenderSection("header", required: false)
</head>
<body>
    @RenderBody()
    @Html.CDNScript("~/bundles/mobile/activity/js")
    @RenderSection("scripts", required: false)
    @Html.Partial("_SystemTailer")
</body>
</html>



