@using SF.Services.Settings
@using SF.AspNetCore.Mvc

@{ 
    var setting = Html.Setting<CustomServiceSetting>();
}
@if(!string.IsNullOrWhiteSpace(setting.CSQQ))
{
<script type="text/javascript">
    window["CSQQ"] = "@setting.CSQQ";
</script>
}
@* 站长统计 *@
@{ 
	var cnzz = "";// setting.CNZZSiteIdent?.Trim() ??"";
}
@if (!string.IsNullOrEmpty(cnzz))
{
<style>
    span#cnzz_stat_icon_@(cnzz){display:none;}
</style>
<script type="text/javascript">
    var cnzz_protocol = (("https:" == document.location.protocol) ? " https://" : " http://");
    document.write(unescape("%3Cspan id='cnzz_stat_icon_@(cnzz)'%3E%3C/span%3E%3Cscript src='" + cnzz_protocol + "s95.cnzz.com/z_stat.php%3Fid%3D@(cnzz)' type='text/javascript'%3E%3C/script%3E"));
</script>
}